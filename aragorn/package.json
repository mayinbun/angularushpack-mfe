{
  "name": "aragorn",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:single-spa:aragorn",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "build:single-spa:aragorn": "ng build aragorn --prod",
    "serve:single-spa:aragorn": "ng serve --project aragorn --disable-host-check --port 5000 --live-reload false"
  },
  "private": true,
  "remoteLocalUrl": "http://localhost:5000",
  "remoteCdnUrl": "someCdnUrl",
  "dependencies": {
    "@angular-architects/module-federation": "12.4.0",
    "@angular/animations": "12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/forms": "12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/router": "12.1.1",
    "@mayinbun/build-tools": "1.0.0",
    "rxjs": "~6.6.0",
    "single-spa": "5.9.3",
    "single-spa-angular": "5.0.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.0",
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@types/node": "^12.11.1",
    "html-webpack-plugin": "~5.3.2",
    "standalone-single-spa-webpack-plugin": "~2.0.2",
    "typescript": "~4.3.2",
    "webpack-merge": "~5.8.0"
  }
}
